# src/test/blk_pool/TEST9 -- unit test for pmemblk_create
#

. ../unittest/unittest.sh

require_test_type medium

setup
umask 0

#
# TEST9 non-existing file, poolsize < min required size
#
expect_normal_exit ./blk_pool$EXESUFFIX c $DIR/testfile 4096 1 0640

check_no_files $DIR/testfile

check

pass
